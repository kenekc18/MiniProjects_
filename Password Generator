import random
char = "abcdefghijkmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890%$*#?/><"
while True:
    pass_len = int(input("How long should password be?:"))
    pass_count = int(input("How many password should be made?:"))
    for x in range(0, pass_count):
        password = ""
        for x in range(0,pass_len):
            password = password + random.choice(char)
        print(password)
